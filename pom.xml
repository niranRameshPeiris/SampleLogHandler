        <artifactId>wso2</artifactId>
        <version>1</version>
    </parent>

    <groupId>wso2-APIM3.1.0-logHandler</groupId>
    <artifactId>wso2-APIM3.1.0-logHandler</artifactId>
    <version>sample-1.0.0</version>

    <dependencies>
        <dependency>
            <groupId>org.apache.synapse</groupId>
            <artifactId>synapse-core</artifactId>
            <version>2.1.7-wso2v147</version>
        </dependency>

        <dependency>
